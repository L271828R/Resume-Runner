# Resume Runner Configuration Example
# Copy this file to .env and fill in your actual values

# Environment Settings
# Options: production, dev, test
ENVIRONMENT=dev

# AWS S3 Configuration
S3_BUCKET_NAME=your-resume-runner-bucket
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Database Configuration
DATABASE_PATH=database/resume_runner.db

# Application Settings
DEBUG=true
LOG_LEVEL=INFO

# Environment-specific S3 path structure:
# production/ - Live production data
# dev/ - Development with persistent data
# test/ - Testing environment (ephemeral data)